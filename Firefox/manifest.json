{
  "manifest_version": 3,
  "name": "AI Prompt Repo",
  "version": "1.21.12",
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/dialog.js",
        "js/drag.js",
        "prompt-repo-helper.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "tmpl/sidebar.html",
        "css/button.css",
        "css/sidebar.css",
        "img/*.svg",
        "js/dialog.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{76861276-4ba2-456b-bcc2-641b7c87d9d2}",
      "strict_min_version": "89.0"
    }
  }
}
